{
  "manifest_version": 2,
  "name": "RemoveHabrCompanies",
  "version": "1.0",

  "description": "Удаление корпоративных блогов из ленты хабра",
  "browser_action": {
    "default_icon": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.habr.com/*"],
      "js": ["remove.js"]
    }
  ]
}
